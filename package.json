{
  "name": "azure-services-map",
  "version": "0.1.0",
  "description": "A visual representation of Azure services",
  "author": "Anton Romanov",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run prebuild; vue-cli-service build",
    "prebuild": "rm -f azure-services-tmp.html && rm -f azure-services-availability-tmp.html && npm run get-services-data && npm run get-services-data-availability",
    "lint": "vue-cli-service lint",
    "get-services-data": "node ./parse-services.js",
    "get-services-data-availability": "node ./parse-services-availability.js"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "0.0.38",
    "axios": "^0.18.1",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.26",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.2",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "puppeteer": "^1.18.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  }
}
